b = input('digite algo:')
print(' o tipo primitivo é',type(b))
print('só tem espaços?', b.isspace())
print('é um numero?',b.isnumeric())
print('é alfabeto?',b.isalpha())
print('é alfanumerico?',b.isalnum())
print('está em maiusculo?',b.isupper())
print('está em minusculo?',b.islower())
print('está capitalizada?',b.istitle())
